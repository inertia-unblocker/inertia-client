{
	"presets": ["next/babel"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@components": "./src/components",
					"@theme": "./src/theme",
					"@utils": "./src/utils",
					"@hooks": "./src/utils/hooks",
					"@css": "./src/css",
					"@config": "./src/config/config.json"
				}
			}
		]
	],
	"ignore": "**/node_modules/**"
}