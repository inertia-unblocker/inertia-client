{
	"presets": ["next/babel"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./"],
				"alias": {
					"@components": "./src/components",
					"@theme": "./src/theme",
					"@cards": "./src/components/cards",
					"@utils": "./src/utils",
					"@hooks": "./src/utils/hooks",
					"@repoRoot": "./"
				}
			}
		]
	],
	"ignore": "**/node_modules/**"
}
	